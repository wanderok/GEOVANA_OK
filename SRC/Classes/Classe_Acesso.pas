procedure TAtualizador.GeraCFOP(pCodigo, pDescricao, pComentario, pForaUF, pES,
  pTipo: String);
begin
    DM.Query1.Close;
    DM.Query1.Sql.Clear;
    DM.Query1.SQL.Add('INSERT INTO NATUREZADEOPERACAO_NATOP                             ');
    DM.Query1.SQL.Add(' (CODIGO, DESCRICAO, DESCRICAO2, FORAESTADO, ES, NAT_TIPO)       ');
    DM.Query1.SQL.Add('VALUES                                               ');
    DM.Query1.SQL.Add(' (:CODIGO, :DESCRICAO, :DESCRICAO2, :FORAESTADO, :ES, :NAT_TIPO)  ');
    DM.Query1.ParamByName('CODIGO'    ).AsString := pCodigo;
    DM.Query1.ParamByName('DESCRICAO' ).AsString := COPY(fTiraAcentos(UPPERCASE(pDescricao)),1,255);
       if copy(pCodigo,1,1) = '1' then
          begin
             // ENTRADA DE MERCADORIA DENTRO DA UF
          DM.Query1.ParamByName('DESCRICAO2').AsString := fTiraAcentos(pComentario)+'(MESMA UF)';
          DM.Query1.ParamByName('FORAESTADO').AsString := 'N';
          DM.Query1.ParamByName('ES'        ).AsString := 'E';
          end
       else
          if copy(pCodigo,1,1) = '2' then
             begin
                // ENTRADA DE MERCADORIA FORA DA UF
             DM.Query1.ParamByName('DESCRICAO2').AsString := UPPERCASE(pComentario)+'(OUTRA UF)';
             DM.Query1.ParamByName('FORAESTADO').AsString := 'S';
             DM.Query1.ParamByName('ES'        ).AsString := 'E';
             end
          else
             if copy(pCodigo,1,1) = '3' then
                begin
                   // ENTRADA DE MERCADORIA DO EXTERIOR
                DM.Query1.ParamByName('DESCRICAO2').AsString := pComentario+'(EXTERIOR)';
                DM.Query1.ParamByName('FORAESTADO').AsString := 'S';
                DM.Query1.ParamByName('ES'        ).AsString := 'E';
                end
             else
                if copy(pCodigo,1,1) = '5' then
                   begin
                      // SAIDA DE MERCADORIA DENTRO DA UF
                   DM.Query1.ParamByName('DESCRICAO2').AsString := pComentario+'(MESMA UF)';
                   DM.Query1.ParamByName('FORAESTADO').AsString := 'N';
                   DM.Query1.ParamByName('ES'        ).AsString := 'S';
                   end
                else
                   if copy(pCodigo,1,1) = '6' then
                      begin
                         // SAIDA DE MERCADORIA FORA DA UF
                      DM.Query1.ParamByName('DESCRICAO2').AsString := pComentario+'(OUTRA UF)';
                      DM.Query1.ParamByName('FORAESTADO').AsString := 'S';
                      DM.Query1.ParamByName('ES'        ).AsString := 'S';
                      end
                   else
                      if copy(pCodigo,1,1) = '6' then
                         begin
                            // SAIDA DE MERCADORIA PARA O EXTERIOR
                         DM.Query1.ParamByName('DESCRICAO2').AsString := pComentario+'(EXTERIOR)';
                         DM.Query1.ParamByName('FORAESTADO').AsString := 'S';
                         DM.Query1.ParamByName('ES'        ).AsString := 'S';
                         end
                      else
                         begin
                            // outras situacoes...
                         DM.Query1.ParamByName('DESCRICAO2').AsString := pComentario;
                         DM.Query1.ParamByName('FORAESTADO').AsString := pForaUF;
                         DM.Query1.ParamByName('ES'        ).AsString := pES;
                         end;


    DM.Query1.ParamByName('NAT_TIPO'  ).AsString := pTipo; // 'VI','VE','CP','DV','DC';
    DM.Query1.ExecSql;
end;

