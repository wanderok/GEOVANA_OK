procedure TAtualizador.AvaliaCrescimentoDoBancoDeDados;
var
  vTabelas,
  vRegistros: Integer;
  Q1, Q2, Q3: TFDQuery;
  vHora,
  vDataAAMM,
  vDataMMAA : String;
  vData     : TDateTime;
begin
    Q1 := TFDQuery.Create(nil);
    Q1.ConnectionName := 'X';

    Q2 := TFDQuery.Create(nil);
    Q2.ConnectionName := 'X';

    Q3 := TFDQuery.Create(nil);
    Q3.ConnectionName := 'X';

    vData     := DataServidor;
    vDataAAMM := DataAAMM(vData);
    vDataMMAA := DataMMAA(vData);

   try
      Q3.close;
      Q3.sql.clear;
      Q3.sql.add('CREATE TABLE TABELAS_TAB               ');
      Q3.sql.add('     (                                 ');
      Q3.sql.add('       TAB_DATA      DATETIME    NULL, ');
      Q3.sql.add('       TAB_HR        VARCHAR(05) NULL, ');
      Q3.sql.add('       TAB_TABELA    VARCHAR(30) NULL, ');
      Q3.sql.add('       TAB_REGISTROS INTEGER     NULL, ');
      Q3.sql.add('       TAB_MESANO    VARCHAR(05) NULL, ');
      Q3.sql.add('       TAB_ANOMES    VARCHAR(05) NULL  ');
      Q3.sql.add('     )                                 ');
      Q3.execsql;
   except
   end;

   if not Ja_Executou_Script('REGISTROSBANCO_RB') then
   begin
       Q3.Close;
       Q3.Sql.Clear;
       Q3.Sql.Add('CREATE TABLE REGISTROSBANCO_RB( ');
       Q3.Sql.Add('	  RB_DATA      datetime NULL, ');
       Q3.Sql.Add('	  RB_REGISTROS int      NULL  ');
       Q3.Sql.Add(')                               ');
       Q3.ExecSql;
   end;

    Q1.close;
    Q1.Sql.Clear;
    Q1.Sql.Add('DELETE REGISTROSBANCO_RB');
    Q1.Sql.Add('  WHERE RB_DATA = :RB_DATA');
    Q1.ParamByName('RB_DATA').AsDateTime := DataServidor;
    Q1.ExecSql;

    Q1.close;
    Q1.Sql.Clear;
    Q1.Sql.Add('SELECT SUM(TAB_REGISTROS) AS TAB_REGISTROS ');
    Q1.Sql.Add('  FROM TABELAS_TAB                         ');
    Q1.Open;

    Q2.close;
    Q2.Sql.Clear;
    Q2.Sql.Add('INSERT INTO REGISTROSBANCO_RB ');
    Q2.Sql.Add('     ( RB_DATA,               ');
    Q2.Sql.Add('       RB_REGISTROS)          ');
    Q2.Sql.Add('VALUES                        ');
    Q2.Sql.Add('     (:RB_DATA,               ');
    Q2.Sql.Add('      :RB_REGISTROS)          ');
    Q2.ParamByName('RB_DATA'     ).AsDateTime := DataServidor;
    Q2.ParamByName('RB_REGISTROS').AsInteger  := Q1.FieldByName('TAB_REGISTROS').AsInteger;
    Q2.ExecSql;

   vHora := HoraServidor;
   vData := DataServidor;


    try
      Q3.close;
      Q3.sql.clear;
      Q3.sql.add('DELETE TABELAS_TAB          ');
      Q3.sql.add(' WHERE TAB_DATA = :TAB_DATA ');
      Q3.ParamByName('TAB_DATA').AsDateTime := vData;
      Q3.execsql;
    except
    end;

    TRY
        Q1.close;
        Q1.sql.clear;
        //Q1.sql.add('select name from sysobjects where type=''u''');
        Q1.sql.add('SELECT TABLE_NAME                    ');
        Q1.sql.add('  FROM information_schema.tables     ');
        Q1.sql.add(' WHERE TABLE_TYPE   = ''BASE TABLE'' ');
        Q1.sql.add('   AND TABLE_SCHEMA = ''dbo''        ');
        Q1.sql.add(' ORDER BY TABLE_name                 ');
        Q1.open; //PausaDelay;
        vTabelas:=0;
        vRegistros := 0;
        while not Q1.eof do
        begin
           inc(vTabelas);
           Q2.close;
           Q2.sql.clear;
           Q2.sql.add('select count(*) as qtde   ');
           Q2.sql.add('  from '+Q1.Fields[0].Value); // ByName('TABLE_NAME').AsString);
           Q2.open; //PausaDelay;
           vRegistros := vRegistros + Q2.fieldByName('qtde').AsInteger;
           try
              Q3.close;
              Q3.sql.clear;
              Q3.sql.add('INSERT INTO TABELAS_TAB  ');
              Q3.sql.add('     (                   ');
              Q3.sql.add('       TAB_DATA,         ');
              Q3.sql.add('       TAB_HR,           ');
              Q3.sql.add('       TAB_TABELA,       ');
              Q3.sql.add('       TAB_REGISTROS,    ');
              Q3.sql.add('       TAB_MESANO,       ');
              Q3.sql.add('       TAB_ANOMES        ');
              Q3.sql.add('     )                   ');
              Q3.sql.add('VALUES                   ');
              Q3.sql.add('     (                   ');
              Q3.sql.add('      :TAB_DATA,         ');
              Q3.sql.add('      :TAB_HR,           ');
              Q3.sql.add('      :TAB_TABELA,       ');
              Q3.sql.add('      :TAB_REGISTROS,    ');
              Q3.sql.add('      :TAB_MESANO,       ');
              Q3.sql.add('      :TAB_ANOMES        ');
              Q3.sql.add('     )                   ');
              Q3.ParamByName('TAB_DATA'           ).AsDateTime := vData;
              Q3.ParamByName('TAB_HR'             ).AsString   := vHora;
              Q3.ParamByName('TAB_TABELA'         ).AsString   := Copy(Q1.Fields[0].Value,1,30); // Q1.fieldbyname('NAME').AsString,1,30);
              Q3.ParamByName('TAB_REGISTROS'      ).AsInteger  := Q2.fieldByName('qtde').AsInteger;
              Q3.ParamByName('TAB_MESANO'         ).AsString   := vDataMMAA;
              Q3.ParamByName('TAB_ANOMES'         ).AsString   := vDataAAMM;
              Q3.execsql;
           except
           end;
           Q1.next;
        end;
    except
    end;
    Q1.Free;
    Q2.Free;
    Q3.Free;
end;

